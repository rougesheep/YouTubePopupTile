{
  "manifest_version": 2,

  "name": "YouTube Popup Tile",
  "description": "Opens the current YouTube video in a fullscreen window.",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "background": {"scripts": ["background.js"] },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "YouTube Tile"
  }
}